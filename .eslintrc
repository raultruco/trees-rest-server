{
  "parser": "babel-eslint",
  "plugins": [
    "import",
    "jest"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "rules": {
    "jest/no-focused-tests": 2,
    "jest/no-identical-title": 2
  }
}
